<%= stylesheet_link_tag    'welcome', media: 'all', 'data-turbolinks-track' => true %>

<div class="row">
  <%= form_tag sessions_path do %>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
    <%= label_tag :password %>
    <%= password_field_tag :password %><br />
    <%= submit_tag "Log In" %>
  <% end %>
</div>
